    "keywords"    : ["geo", "gps", "trajectory"],
    "homepage"    : "https://github.com/alfechner/geotrajectory",
    "license"     : "MIT",

    "authors" : [{
        "name"     : "Alex Fechner",
        "email"    : "fechner.alex@gmail.com",
        "homepage" : "http://www.alexfechner.de",
        "role"     : "Developer"
    }],

    "support" : {
        "email"  : "fechner.alex@gmail.com",
        "issues" : "https://github.com/alfechner/geotrajectory/issues",
        "source" : "https://github.com/alfechner/geotrajectory"
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alfechner/gpstrajectory.git"
        }
    ],

    "require" : {
        "php"                               : ">=5.5.9",
        "triplepoint/php-units-of-measure"  : "dev-master"
